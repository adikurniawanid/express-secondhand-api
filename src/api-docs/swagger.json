{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Game REST API",
    "description": "User Game Rest Api Information"
  },
  "host": "127.0.0.1:3000",
  "basePath": "/",
  "schemes": [
    "http",
    "https"
  ],
  "tags":
  [
    {
      "name": "Login",
      "description": "Login operation"
    },
    {
      "name": "User",
      "description": "Everyting about User Game || Need authorizations*"
    }
  ],
  "paths": {
    "/login/": {
      "post": {
        "tags": [
          "Login"
        ],
        "summary": "Get token",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "example": "admin"
                },
                "password": {
                  "example": "admin"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties":{
                    "token": {
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJwdWJsaWNJZCI6IjllNTBhYjM5LThmOTktNDMzMy1iMmI0LThiNjM0NGJkMjNiNiIsInVzZXJuYW1lIjoiYWRtaW4iLCJpYXQiOjE2NTA1NDg3OTl9.B_rvoZwvULTQEKooEG1tcAwnw52fhQvHMss2KshshU4"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Invalid username or password"
                }
              }
            }
          }
        }
      }
    },
    "/users/": {
      "get": {
        "tags": [
          "User"
        ],
        "summary": "Get user list",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "publicId": {
                    "example": "9e50ab39-8f99-4333-b2b4-8b6344bd23b6"
                  },
                  "username": {
                    "example": "admin"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Unauthorized User"
                }
              }
            }
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      },
      "post": {
        "tags": [
          "User"
        ],
        "summary": "Add user",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "example": "akdev"
                },
                "password": {
                  "example": "akdev"
                },
                "name": {
                  "example": "Adi Kurniawan"
                },
                "age": {
                  "example": 22
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Success add user"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Unauthorized"
                }
              }
            }
          },
          "422": {
            "description": "Unprocessable Entity",
            "schema": {
              "type": "object",
              "properties": {
                "errors": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "username": {
                        "example": "Username already in use"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    },
    "/users/{id}": {
      "get": {
        "tags": [
          "User"
        ],
        "summary": "Get detail user information",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "publicId": {
                      "example": "4fe2b5c0-7bad-4db3-93d9-afd5e6ce792f"
                      
                    }
                    ,
                    "username": {
                      "example":"SamanthaHeathcote"},
                      "name": {
                        "example":"Samantha Heathcote"},
                      "age": {
                        "example": 32}
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Unauthorized"
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "User not found"
                }
              }
            }
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      },
      "put": {
        "tags": [
          "User"
        ],
        "summary": "Update user information",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "example": "GEGEWEPE ADI"
                },
                "age": {
                  "example": "18"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Success update user"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Unauthorized"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "User not found"
                }
              }
            }
          },
          "422": {
            "description": "Unprocessable Entity",
            "schema": {
              "type": "object",
              "properties": {
                "errors": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "username": {
                        "example": "Username already in use"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      },
      "delete": {
        "tags": [
          "User"
        ],
        "summary": "Delete user information",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Success delete user"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "Unauthorized"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "example": "User not found"
                }
              }
            }
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    }
  },
  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  },
  "definitions": {
    "UserGames": {
      "required": [
        "id",
        "publicId",
        "username",
        "password",
        "createdAt",
        "updatedAt"
      ],
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "publicId": {
          "type": "uuid"
        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "createdAt": {
          "type": "date"
        },
        "updatedAt": {
          "type": "date"
        },
        "deletedAt": {
          "type": "date"
        }
      },
      "xml": {
        "name": "Tag"
      }
    },
    "UserGameBiodatas": {
      "required": [
        "userId",
        "name",
        "username",
        "password",
        "createdAt",
        "updatedAt"
      ],
      "type": "object",
      "properties": {
        "userId": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "age": {
          "type": "integer"
        },
        "createdAt": {
          "type": "date"
        },
        "updatedAt": {
          "type": "date"
        }
      },
      "xml": {
        "name": "Tag"
      }
    },
    "UserGameHistories": {
      "required": [
        "id",
        "userId",
        "startTime",
        "endTime",
        "score",
        "createdAt",
        "updatedAt"
      ],
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "userId": {
          "type": "integer"
        },
        "startTime": {
          "type": "date"
        },
        "endTime": {
          "type": "date"
        },
        "score": {
          "type": "biginteger"
        },
        "createdAt": {
          "type": "date"
        },
        "updatedAt": {
          "type": "date"
        }
      },
      "xml": {
        "name": "Tag"
      }
    }
  }
}